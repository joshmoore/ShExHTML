PREFIX sx: <http://www.w3.org/ns/shex#>

[]
  a sx:Schema ;
  sx:shapes ( <S> ) .

<S>
  a <http://www.w3.org/ns/shex#Shape> ;
  sx:expression [
    a sx:TripleConstraint ;
    sx:predicate <p> ;
    sx:valueExpr [
      a sx:NodeConstraint ;
      sx:semActs ( [
        a sx:SemAct ;
        sx:name <lang>
      ] ) ;
      sx:values ( 1 )
    ]
  ] .
